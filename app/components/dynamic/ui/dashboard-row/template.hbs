<div local-class="dashboard_row_header">
  <div local-class="dashboard_row_header_title">
    {{@projectTitle}}
  </div>
  <div local-class="dashboard_row_header_description">
    {{@projectDescription}}
  </div>
</div>
<div local-class="dashboard_row_container">
  <div local-class="dashboard_row">
    {{#if @blockOneImage}}
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_image">
        <img src="{{@blockOneImage}}" alt="Project Cover" />
      </div>
    </div>
    {{/if}}
    {{#if @iconOne}}
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_icon_container">
        {{#if @iconOne}}
          <div local-class="dashboard_block_icon">
            <img src="{{@iconOne}}" alt="icon">
          </div>
        {{/if}}
        {{#if @iconTwo}}
        <div local-class="dashboard_block_icon">
          <img src="{{@iconTwo}}" alt="icon">
        </div>
        {{/if}}
        {{#if @iconThree}}
        <div local-class="dashboard_block_icon">
          <img src="{{@iconThree}}" alt="icon">
        </div>
        {{/if}}
      </div>
      <div local-class="dashboard_block_label">
        {{@blockTwoLabel}}
      </div>
    </div>
    {{/if}}
    {{#if @blockThreeTextOne}}
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_text">
        {{@blockThreeTextOne}} <br />
        {{@blockThreeTextTwo}} <br />
        {{@blockThreeTextThree}}
      </div>
      <div local-class="dashboard_block_label">
        {{@blockThreeLabel}}
      </div>
    </div>
    {{/if}}
    {{#if @blockFourTextOne}}
    <div local-class="dashboard_block">
      <div local-class="dashboard_block_text">
        {{@blockFourTextOne}} <br />
        {{@blockFourTextTwo}} <br />
        {{@blockFourTextThree}}
      </div>
      <div local-class="dashboard_block_label">
        {{@blockFourLabel}}
      </div>
    </div>
    {{/if}}
    {{#if @projectGithubUrl}}
    <a href="{{@projectGithubUrl}}" target="_blank" rel="noopener noreferrer">
      <div local-class="dashboard_block">
        <div local-class="dashboard_block_icon_container">
          <div local-class="dashboard_block_icon">
            <FaIcon @icon="github-alt" @prefix='fab' @size='3x' />
          </div>
        </div>
        <div local-class="dashboard_block_label">
          {{@blockFiveLabel}}
        </div>
      </div>
    </a> 
    {{/if}}
    {{#if @projectLiveSiteUrl}}     
    <a href="{{@projectLiveSiteUrl}}" target="_blank" rel="noopener noreferrer">
      <div local-class="dashboard_block">
        <div local-class="dashboard_block_icon_container">
        <div local-class="dashboard_block_text">
          {{@blockSixText}}
        </div>
        </div>
        <div local-class="dashboard_block_label">
          {{@blockSixLabel}}
        </div>
      </div>
    </a>
    {{/if}}
  </div>
  {{!-- <div local-class="arrow">
    >
  </div> --}}
</div>